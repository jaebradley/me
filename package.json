{
  "name": "me",
  "version": "0.0.0-development",
  "private": true,
  "homepage": "http://jaebradley.github.io/me",
  "repository": "https://github.com/jaebradley/me.git",
  "description": "Personal website",
  "main": "index.js",
  "author": "Jae Bradley <jae.b.bradley@gmail.com>",
  "license": "MIT",
  "gh-pages-deploy": {
    "staticpath": "public",
    "prep": [
      "build"
    ],
    "noprompt": true
  },
  "dependencies": {
    "@material-ui/core": "^1.4.0",
    "@material-ui/icons": "^1.1.0",
    "classnames": "^2.2.6",
    "object-assign": "^4.1.1",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-fontawesome": "^1.6.1",
    "react-router-dom": "^4.3.1",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/core": "^7.0.0-beta.54",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.54",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/preset-react": "^7.0.0-beta.54",
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-angular": "^7.0.1",
    "@commitlint/prompt": "^7.0.0",
    "@commitlint/prompt-cli": "^7.0.0",
    "autoprefixer": "^9.0.0",
    "babel-loader": "^8.0.0-beta",
    "babel-plugin-import": "^1.8.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^0.1.19",
    "codecov": "^3.0.0",
    "css-loader": "1.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "fs-extra": "^7.0.0",
    "gh-pages": "^1.1.0",
    "gh-pages-deploy": "^0.5.0",
    "hard-source-webpack-plugin": "^0.11.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^23.4.1",
    "name-all-modules-plugin": "^1.0.1",
    "postcss-flexbugs-fixes": "^3.3.1",
    "postcss-loader": "^2.1.6",
    "react-dev-utils": "^6.0.0-next.3e165448",
    "react-error-overlay": "^5.0.0-next.3e165448",
    "react-hot-loader": "^4.3.3",
    "react-scripts": "^2.0.0-next.3e165448",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.1",
    "webpack-build-notifier": "^0.1.28",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "3.1.4",
    "webpack-manifest-plugin": "^2.0.3",
    "webpack-merge": "^4.1.3"
  },
  "scripts": {
    "start": "PORT=3002 node scripts/start",
    "build-assets": "node scripts/build.js",
    "build": "node scripts/build",
    "test": "node scripts/test.js --env=jsdom",
    "linter": "node scripts/linter.js"
  },
  "jest": {
    "globals": {
      "$": "<rootDir>/node_modules/jquery/dist/jquery.js",
      "window": {}
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/jest/mockLocalStorage.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/scripts/setupTest.js",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!lodash-es)"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "\\.(css)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  }
}
